<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.com.patitas">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".veteriapp.VeteriApp"
        android:theme="@style/AppTheme">
        <activity android:name=".veteriapp.activities.NewOrderActivity"></activity>
        <activity android:name=".veteriapp.activities.NewPetActivity" />
        <activity android:name=".veteriapp.activities.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".veteriapp.activities.LoginActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".veteriapp.activities.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>